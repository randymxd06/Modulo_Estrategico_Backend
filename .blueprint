created:
    - database/seeders/ProductCategorySeeder.php
    - database/seeders/ProductSeeder.php
models:
    User: { fullname: string, email: 'string:100', password: string, remember_token: 'string:100 nullable' }
    ProductCategory: { id: id, name: 'string:40', description: text, status: 'boolean default:true', timestamps: timestamps, softdeletes: softDeletes, relationships: { belongsTo: Product } }
    Product: { id: id, name: 'string:100', description: text, product_category_id: 'id foreign:product_categories.id', price: 'decimal:8,2', image_url: 'string nullable', estimated_time: time, score: integer, status: 'boolean default:true', timestamps: timestamps, softdeletes: softDeletes, relationships: { belongsTo: 'OrderProduct, Recipe, MenusVsProduct', hasMany: ProductCategory } }
